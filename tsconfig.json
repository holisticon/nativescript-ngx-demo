{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": true,
        "pretty": true,
        "removeComments": true,
        "experimentalDecorators": true,
        "noEmitHelpers": false,
        "noEmitOnError": true,
        "skipDefaultLibCheck": true,
        "skipLibCheck": true,
        "noLib": false,
        "importHelpers": false,
        "noImplicitAny": false,
        "noImplicitReturns": true,
        "noImplicitUseStrict": false,
        "noFallthroughCasesInSwitch": true,
        "lib": [
            "dom",
            "es5",
            "es2015",
            "es2015.iterable",
            "es6"
        ],
        "types": [
            "jasmine"
        ],
        "baseUrl": ".",
        "paths": {
            "*": [
                "./node_modules/tns-core-modules/*",
                "./node_modules/*"
            ]
        }
    },
    "files": [
        "references.d.ts"
    ],
    "exclude": [
        "node_modules",
        "platforms",
        "**/*.aot.ts"
    ]
}